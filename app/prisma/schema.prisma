  deductible        Float?
  premium           Float          // Annual premium
  
  // Policy period
  effectiveDate     DateTime
  expirationDate    DateTime
  autoRenewal       Boolean        @default(false)
  
  // Claims and usage
  claimsHistory     Json?          // Historical claims
  claimsCount       Int            @default(0)
  lastClaimDate     DateTime?
  
  // Risk assessment
  riskCovered       Json?          // Types of risks covered
