// Advanced Reporting Engine
model CustomReport {
  id              String           @id @default(cuid())
  userId          String
  name            String
  description     String?
  category        ReportCategory
  
  // Report configuration
  reportType      ReportType
  dataSource      Json             // Data sources and tables
  filters         Json             // Report filters
  grouping        Json?            // How to group data
  sorting         Json?            // Sorting preferences
  
  // Visualization
  chartType       ChartType?
  visualization   Json?            // Chart/visualization config
  
  // Output format
  outputFormat    OutputFormat     @default(PDF)
  pageLayout      PageLayout       @default(PORTRAIT)
  
  // Scheduling
  isScheduled     Boolean          @default(false)
  schedule        Json?            // Cron schedule
  recipients      Json?            // Email recipients
  lastGenerated   DateTime?
  nextGeneration  DateTime?
  
  // Template
  template        Json?            // Report template
  
  isActive        Boolean          @default(true)
  createdAt       DateTime         @default(now())
  updatedAt       DateTime         @updatedAt

  user            User             @relation(fields: [userId], references: [id], onDelete: Cascade)
  generations     ReportGeneration[]

  @@index([userId, category])
  @@index([userId, reportType])
  @@index([userId, isActive])
}

model ReportGeneration {
  id              String       @id @default(cuid())
  reportId        String
  
  // Generation details
  generatedAt     DateTime     @default(now())
  parameters      Json?        // Parameters used for this generation
  
  // Output
  cloudStoragePath String?      // Path to generated report file
  fileSize        Int?
  pageCount       Int?
  
  // Status and performance
  status          GenerationStatus @default(PENDING)
  errorMessage    String?
  executionTime   Int?         // Milliseconds
  
  // Distribution
  emailsSent      Int          @default(0)
  downloadCount   Int          @default(0)
  
  createdAt       DateTime     @default(now())
  updatedAt       DateTime     @updatedAt

  report          CustomReport @relation(fields: [reportId], references: [id], onDelete: Cascade)

  @@index([reportId, generatedAt])
  @@index([status])
}

// Market Intelligence
model MarketData {
  id              String           @id @default(cuid())
  userId          String
  industry        String
  region          String?
  
  // Market metrics
  marketSize      Float?           // Total addressable market
  growthRate      Float?           // Annual growth rate
  
  // Competitive data
  competitors     Json?            // List of competitors
  marketShare     Json?            // Market share data
  
  // Economic indicators
  economicData    Json?            // Relevant economic indicators
  
  // Trends
  trends          Json?            // Market trends
  opportunities   Json?            // Market opportunities
  threats         Json?            // Market threats
  
  // Data source and freshness
  dataSource      String?
  dataDate        DateTime
  confidence      Float?           // Data confidence score
  
  createdAt       DateTime         @default(now())
  updatedAt       DateTime         @updatedAt

  user            User             @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, industry, region])
  @@index([userId, industry])
  @@index([dataDate])
}

model CompetitiveAnalysis {
  id                String       @id @default(cuid())
  userId            String
  competitorName    String
  industry          String
  
  // Company details
  description       String?
  website           String?
  employees         Int?
  foundedYear       Int?
  
  // Financial data (if available)
  estimatedRevenue  Float?
  fundingRaised     Float?
  valuation         Float?
  
  // Competitive positioning
  strengths         Json?
  weaknesses        Json?
  marketPosition    String?
  targetMarket      Json?
  
  // Product/service comparison
  products          Json?        // Their product offerings
  pricing           Json?        // Pricing information
  features          Json?        // Feature comparison
  
  // Analysis
  threatLevel       ThreatLevel  @default(MEDIUM)
  competitiveGap    Json?        // Where we're ahead/behind
  recommendations   Json?        // Strategic recommendations
  
  // Data freshness
  lastUpdated       DateTime
  dataSource        String?
  
  isActive          Boolean      @default(true)
  createdAt         DateTime     @default(now())
  updatedAt         DateTime     @updatedAt

  user              User         @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, competitorName])
  @@index([userId, industry])
  @@index([userId, threatLevel])
}

